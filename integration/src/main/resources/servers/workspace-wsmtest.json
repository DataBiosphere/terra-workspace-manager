  "workspaceManagerUri": "https://workspace.wsmtest.integ.envs.broadinstitute.org/",

  "cluster": {
    "clusterName": "gke_terra-kernel-k8s_us-central1-a_terra-integration",
    "clusterShortName": "terra-integration",
