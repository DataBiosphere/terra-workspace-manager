    "project": "terra-kernel-k8s",
    "namespace": "terra-wsmtest",
    "containerName": "workspacemanager-deployment",
    "apiComponentLabel":  "workspacemanager"
  },
  "deploymentScript": {},
  "testRunnerServiceAccountFile": "testrunner-sa.json",
  "testRunnerK8SServiceAccountFile": "testrunner-k8s-wsmtest-sa.json",

  "skipDeployment": true,
  "skipKubernetes": false,

  "versionScripts": [
    {
      "name": "ReadFromTerraHelmFileRepoAndGitCommitLog",
      "description": "Version from https://github.com/broadinstitute/terra-helmfile and Git Commit Log",
      "parameters": ["terra-helmfile/versions/app/dev.yaml", "terra-helmfile/environments/personal/wsmtest.yaml", "../.git"]
    }
  ]
